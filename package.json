{
  "name": "tts-asc-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider --max_old-space_size=4096 && ng serve --ssl --proxy-config proxy.conf.json",
    "start-server:dev": "set NODE_ENV=dev&&set PORT=3000&& node app.js",
    "start-server:test": "set NODE_ENV=test2&& node app.js",
    "build": "node --max_old_space_size=4096 --openssl-legacy-provider ./node_modules/@angular/cli/bin/ng build --source-map=false",
    "build-prod": "node --max_old_space_size=4096 --openssl-legacy-provider ./node_modules/@angular/cli/bin/ng build --source-map=false --configuration production",
    "test": "ng test",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && git add -A ."
    }
  },
  "private": true,
